!!!
html
  head
    title #{title} | Compart.ir
    link(rel="icon", type="image/x-icon", href="/images/favicon.ico")
    link(rel="shortcut icon", type="image/x-icon", href="/images/favicon.ico")
    meta(name="viewport", content="initial-scale=1.0, user-scalable=no")
    meta(http-equiv="content-type", content="text/html; charset=UTF-8")  
    script(src="/js/libs/jquery.min.js")
    - if (typeof(styles) != 'undefined')  
      each l in styles
        link(href=l, rel='stylesheet', type='text/css')
    - if (typeof(scripts) != 'undefined') 
      each s in scripts
        script(src=s)  
    link(href='http://fonts.googleapis.com/css?family=Quattrocento+Sans:400,400italic,700,700italic', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href='/stylesheets/style.css')
  body(class="#{classes}")
    header
      .inner
        a(href="/", id="logo")
          img(src="/images/logo.png", title="compart.ir")
        nav#header-menu
          ul
            li.first
              &nbsp;
            if user
              li
                a(href="/profile") #{user.matricula}
              li
                a(href="/profile") #{user.name}
              li
                a(href="/logout") Salir
              li.last
                &nbsp;
              li#nr
                a(href="/new", id="newride") Crear Viaje
            else
              li
                a(href="/login") Iniciar Sesion
              li.last
                &nbsp;
    article#main-content  
      .inner
        h1(id="page-title") #{title}
        #wrapper!= body
    footer
      .inner
        nav#social-menu
          ul
            li.twitter
              a(href='#') Twitter
            li.fb
              a(href='#') Facebook
        nav#company-menu
          ul
            li
              a(href='#') Preguntas frecuentes
            li
              a(href='#') TÃ©rminos de uso 
            li
              a(href='#') Aviso de privacidad
            li
              a(href='#') Contacto
        
